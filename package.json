{
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "moment-timezone": "^0.5.43",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.4"
  },
  "scripts": {
    "dev": "nodemon ./app/server.ts",
    "start": "ts-node ./app/server.ts"
  },
  "nodemonConfig": {
    "exec": "ts-node app/server.ts",
    "watch": [
      "app/**/*.ts"
    ],
    "ext": "ts,json",
    "delay": "3000"
  },
  "lint-staged": {
    "app/**/*.{json,ts}": [
      "eslint"
    ],
    "app/**/*.ts": [
      "tsc-files --noEmit"
    ]
  },
  "devDependencies": {
    "@swc/core": "^1.3.76",
    "@swc/helpers": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.10",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
