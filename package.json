{
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@types/ws": "^8.5.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "moment-timezone": "^0.5.43",
    "socket.io": "^4.7.2",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.4",
    "ws": "^8.13.0",
    "wscat": "^5.2.0"
  },
  "scripts": {
    "dev": "nodemon ./app/server.ts",
    "start": "ts-node ./app/server.ts",
    "test": "jest"
  },
  "nodemonConfig": {
    "exec": "ts-node app/server.ts",
    "watch": [
      "app/**/*.ts",
      "app/public/index.html"
